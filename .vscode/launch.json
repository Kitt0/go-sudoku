{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": ["-filename=samples/800wi.png"],
            "showLog": true
        },
        {
            "name": "Test",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "remotePath": "",
            "port": 2345,
            "host": "127.0.0.1",
            "program": "${fileDirname}",
            "env": {},
            "args": [],
            "showLog": true
        },
        {
            "name": "C++ Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceRoot}/sudokuparser/sudokuparser",
            "args": [
                "train",
                "train_config.csv"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceRoot}/sudokuparser/",
            "environment": [
                { 
                    "name": "DYLD_LIBRARY_PATH",
                    "value": "/usr/lib:/Users/jandersen/anaconda3/envs/opencvenv/lib"
                }
            ],
            "externalConsole": true,
            "preLaunchTask": "g++",
            "linux": {
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ]
            },
            "osx": {
                "MIMode": "lldb"
            },
            "windows": {
                "MIMode": "gdb",
                "setupCommands": [
                    {
                        "description": "Enable pretty-printing for gdb",
                        "text": "-enable-pretty-printing",
                        "ignoreFailures": true
                    }
                ]
            }
        }
    ]
}